<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.matsupy.cmn.mapper.GoodsMapperExt">
  <select id="selGoods" resultType="com.matsupy.cmn.entity.gen.GoodsTbl">
    SELECT
        *
    FROM
        goods
    <where>
	    <if test="goodsId != null">
	        goods_id = #{goodsId}
	    </if>
        AND deleted_flag = #{deletedFlag}
    </where>
  </select>
</mapper>